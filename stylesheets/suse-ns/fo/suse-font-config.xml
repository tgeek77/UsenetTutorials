<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    SUSE Font Configuration File
-->
<fontconfig version="1.0"
      xmlns="http://www.suse.de/ns/fontconfig">
  <formatter name="xep">
   <languages langs="#default">
    <fonts type="body.font"></fonts>
    <fonts type="sans.font"></fonts>
    <fonts type="monospace.font"></fonts>
    <fonts type="symbol.font">Symbol,ZapfDingbats</fonts>
    <fonts type="dingbat.font"></fonts>
    <fonts type="callout.font">ZapfDingbats</fonts>
   </languages>
   <languages langs="ar">
    <fonts type="body.font"></fonts>
    <fonts type="sans.font"></fonts>
    <fonts type="monospace.font"></fonts>
    <fonts type="symbol.font"></fonts>
    <fonts type="dingbat.font"></fonts>
   </languages>
   <languages langs="zh_TW,zh_CW">
    <fonts type="body.font">suse.serif,FreeSerif,Chinese,serif</fonts>
    <fonts type="sans.font">suse.sans,Chinese,FreeSans,sansserif</fonts>
    <fonts type="monospace.font">suse.mono,ChineseMono</fonts>
    <!--<fonts type="symbol.font"></fonts>
    <fonts type="dingbat.font"></fonts>-->
   </languages>
   <languages langs="ko">
    <fonts type="body.font">suse.serif,FreeSerif,KoreanSerif,serif</fonts>
    <fonts type="sans.font">suse.sans,FreeSans,KoreanSans,sansserif</fonts>
    <fonts type="monospace.font">suse.mono</fonts>
    <!--<fonts type="symbol.font"></fonts>
    <fonts type="dingbat.font"></fonts>-->
   </languages>
   <languages langs="ja">
    <fonts type="body.font">suse.serif,FreeSerif,Japanese,serif</fonts>
    <fonts type="sans.font">suse.sans,Japanese,FreeSans,sansserif</fonts>
    <fonts type="monospace.font">suse.mono,JapaneseMono</fonts>
    <!--<fonts type="symbol.font"></fonts>
    <fonts type="dingbat.font"></fonts>-->
   </languages>
  </formatter>
  <formatter name="fop">
   <languages langs="#default">
    <fonts type="body.font">suse_serif</fonts>
    <fonts type="sans.font">suse_sans</fonts>
    <fonts type="monospace.font">suse_mono</fonts>
    <!--<fonts type="symbol.font"></fonts>
    <fonts type="dingbat.font"></fonts>-->
   </languages>   
  </formatter>
</fontconfig>
